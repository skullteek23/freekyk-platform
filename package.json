{
  "name": "football-platform-v1",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port=4200",
    "start-admin": "ng serve -o admin --port=4400",
    "build": "npm run build-dev",
    "build-dev": "ng build admin --configuration development && ng build --configuration development && npm run deploy-dev",
    "build-prod": "ng build admin --configuration production && ng build --configuration production && npm run deploy-prod",
    "deploy": "npm run firebase-deploy-dev && npm run firebase-deploy-prod",
    "deploy-functions-dev": "firebase use freekyk-development && firebase deploy --only functions",
    "deploy-functions-prod": "firebase use freekyk-prod && firebase deploy --only functions",
    "deploy-dev": "firebase use freekyk-development && firebase deploy --only hosting",
    "deploy-prod": "firebase use freekyk-prod && firebase deploy --only hosting",
    "firebase-deploy-dev": "firebase use freekyk-development && firebase deploy",
    "firebase-deploy-prod": "firebase use freekyk-prod && firebase deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.13",

    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ngneat/falso": "^6.1.0",
    "@ngrx/store": "^11.1.1",
    "@ngrx/store-devtools": "^11.1.1",
    "firebase": "^8.5.0",
    "lodash": "^4.17.21",
    "ng-image-slider": "^2.8.0",
